{
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "always"],
    "camelcase": [2, {"properties": "always"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "operator-linebreak": [2, "before"],
    "max-depth": [2, 4],
    "no-lone-blocks": 2,
    "no-lonely-if": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-eval": 2,
    "comma-dangle": 2,
    "no-self-compare": 2,
    "no-unreachable": 2,
    "no-invalid-regexp": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-redeclare": 2,
    "no-unused-vars": 0,
    "default-case": 2,
    "no-new-func": 2,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {"anonymous":"always","named":"never"}]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}