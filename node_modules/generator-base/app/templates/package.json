{
  "name": "generator-base",
  "version": "1.0.0",
  "description": "fdfe base generator",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node webpack.dev.server.js",
    "build:clean": "./node_modules/.bin/rimraf dist rev",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.build.config.js --progress --profile --colors --bail",
    "build:webpack-pre-eploy": "NODE_ENV=production webpack -p --config webpack.preDeploy.config.js --progress --profile --colors --bail",
    "build:html": "node scripts/build-html.js",
    "build:copy": "node scripts/build-copy.js",
    "build:pre-deploy": "npm run build:clean && npm run build:webpack-pre-eploy && gulp build && npm run build:html && npm run build:copy",
    "build": "npm run build:clean && npm run build:webpack && gulp build && npm run build:html && npm run build:copy"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "async": "^1.4.2",
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "classnames": "^2.1.3",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.13.3",
    "express-urlrewrite": "^1.2.0",
    "file-loader": "^0.8.4",
    "flux": "^2.0.0",
    "frep": "^0.2.3",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.14",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-rev": "^6.0.1",
    "gulp-rev-collector": "^1.0.2",
    "gulp-useref": "^1.3.0",
    "history": "^1.11.1",
    "http-proxy": "^1.11.2",
    "json-loader": "^0.5.3",
    "less": "^2.5.1",
    "less-loader": "^2.2.1",
    "node-sass": "^3.3.3",
    "opn": "^3.0.2",
    "postcss-loader": "^0.6.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  },
